SELECT * FROM phone;


SELECT COUNT(phone_price) as count
FROM phone;

SELECT MAX(phone_price) AS maximum
FROM phone;

SELECT MIN(phone_price) AS minimum
FROM phone;


SELECT AVG(phone_price) AS average
FROM phone;

SELECT SUM(phone_price) AS sum
FROM phone;

SELECT CONCAT(first_name," ",last_name) AS full_name
FROM phone;
