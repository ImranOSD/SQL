
CREATE TABLE phone(
imei_id INT PRIMARY KEY AUTO_INCREMENT,
first_name VARCHAR(30),
last_name VARCHAR(30),
phone_name VARCHAR(20),
phone_price DECIMAL(10,2)
);



ALTER TABLE phone
AUTO_INCREMENT = 1002010;




