         CREATE TABLE buyer(
customer_id INT PRIMARY KEY AUTO_INCREMENT,
customer_name VARCHAR(23),
product_price DECIMAL(10,2),
product_id INT,

FOREIGN KEY (product_id) REFERENCES iqoo(product_id)
);

INSERT INTO buyer(customer_name,product_price)
VALUES("Imran",86999.00),
      ("Rahat",56999.00),
      ("Amin",31000.00),
      ("Alim",65000.00),
      ("Jarif",25000.00);
      
      SELECT * FROM buyer;
      
SELECT *
FROM buyer
INNER JOIN iqoo
ON buyer.product_id = iqoo.product_id;


SELECT*FROM buyer;
SELECT *
FROM transactions LEFT JOIN customers
ON transactions.customer_id = customers.customer_id;

SELECT *
FROM transactions RIGHT JOIN customers
ON transactions.customer_id = customers.customer_id;
