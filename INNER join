      CREATE TABLE iqoo(
product_id INT PRIMARY KEY AUTO_INCREMENT,
product_name VARCHAR(20),
product_model VARCHAR(20)
);

ALTER TABLE iqoo
AUTO_INCREMENT = 32010;
